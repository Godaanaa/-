public class AAA {
	public static void main(String[] args) {
		foo("안녕", 1);
		foo("안녕하세요", 1, 2);
		foo("잘 있어");
	}
	static void foo(String a, int... b) {
		System.out.print(a);
		if (b.length > 0) {
			System.out.print(" ");
			for (int num : b) {
				System.out.print(num + " ");
			}
		}
		System.out.println();
	}
}
